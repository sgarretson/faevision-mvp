{
  "name": "faevision-simplified",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "playwright test --project='Performance Testing'",
    "test:accessibility": "playwright test --project='Accessibility Testing'",
    "test:mobile": "playwright test --project='Mobile Chrome - Executive Mobile'",
    "test:workflow": "playwright test executive-workflow.spec.ts",
    "test:install": "playwright install",
    "test:report": "playwright show-report",
    "test:setup": "npm run test:install && npm run db:reset && npm run setup:demo",
    "test:watch": "vitest --watch",
    "deploy:production": "vercel --prod",
    "setup:production-db": "tsx scripts/setup-production-database.ts",
    "validate:production-db": "tsx scripts/setup-production-database.ts --validate-only",
    "deploy:full-production": "npm run setup:production-db && npm run deploy:production",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:preview": "node prisma/preview-seed.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "setup:demo": "node scripts/setup-demo-database.js",
    "setup:comprehensive-seed": "tsx prisma/comprehensive-seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^2.4.3",
    "next": "14.2.32",
    "prisma": "^6.15.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@playwright/test": "^1.40.0",
    "@axe-core/playwright": "^4.8.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.32",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.0.0",
    "typescript": "^5",
    "vitest": "^1.0.0"
  },
  "prisma": {
    "seed": "node prisma/simple-seed.js"
  }
}
